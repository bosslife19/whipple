{
  "expo": {
    "name": "Whipple",
    "slug": "Whipple",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/whipple_logo.jpg",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "jsEngine": "hermes",
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/whipple_logo.jpg",
        "backgroundColor": "#ffffff"
      },
      "package": "com.whipple.whipple",
      "googleServicesFile": "./google-services.json"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/whipple_logo.jpg"
    },
    "plugins": [
      "expo-router",
      "expo-notifications",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/whipple_logo.jpg",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        
        "projectId": "b987a903-6475-4eaf-887f-e5c3b37d725f"
      }
    }
  }
}
